<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:android_custom="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary">


            <com.htc.datausagemonitor.fragment.RoundProgressBar
                android:id="@+id/roundProgressBar"
                android:layout_width="@dimen/RoundProgressBar_Width"
                android:layout_height="@dimen/RoundProgressBar_Height"
                android:layout_alignParentBottom="true"
                android_custom:roundColor="#339dd4"
                android_custom:roundProgressColor="@color/widget_awesome_storage_color"
                android_custom:textColor="@color/widget_awesome_storage_color"
                android_custom:roundWidth="@dimen/RoundProgressBar_roundWidth"
                android_custom:textIsDisplayable="true"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="@dimen/RoundProgressBar_marginTop"/>

            <com.htc.datausagemonitor.fragment.CircleProgressView
                android:id="@+id/circleProgressbar"
                android:background="@drawable/circle"
                android:layout_width="@dimen/CircleProgressView_Width"
                android:layout_height="@dimen/CircleProgressView_Height"
                android:layout_marginTop="@dimen/CircleProgressView_marginTop"
                android:layout_gravity="center_horizontal"

                />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="@dimen/CircleProgressView_text_marginTOP"

           >
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_gravity="center"

                >
                <TextView
                    android:id="@+id/maintraffic"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_gravity="bottom"
                    android:text="0"
                    android:textSize="@dimen/Text_size_Traffic"
                    android:fontFamily="sans-serif-light"
                    android:textStyle="normal"
                />
                <TextView
                    android:id="@+id/maingmkb"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/maintraffic"
                    android:paddingLeft="@dimen/CircleProgressView_text_marginleft"
                    android:layout_alignBaseline="@+id/maintraffic"
                    android:textSize="@dimen/Text_size_GMKB"
                    android:fontFamily="sans-serif-light"
                    android:textStyle="normal"
                    android:text="M"
                    />
                </RelativeLayout>
            <TextView
                android:id="@+id/maininfo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/CircleProgressView_text2_marginTOP"
                android:textSize="@dimen/Text_size_Info"
                android:text="@string/data_used_1"
                />
            <TextView
                android:id="@+id/maintext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:textColor="@color/white"
                android:textSize="@dimen/Text_size_Text"
                android:layout_marginTop="@dimen/CircleProgressView_text3_marginTOP"
                android:text="@string/data_common_used"
                />
            <TextView
                android:id="@+id/subtext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:textColor="@color/white"
                android:text="@string/night_data_title"
                android:textSize="@dimen/Text_size_Text"
                android:layout_marginTop="@dimen/margin_CircleProgressView_text"
                />
            <TextView
                android:id="@+id/subinfo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/CircleProgressView_text3_marginTOP"
                android:text="@string/data_residue_1"
                android:textSize="@dimen/Text_size_Info"

                />
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/CircleProgressView_text2_marginTOP"
                >
                <TextView
                    android:id="@+id/subtraffic"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_gravity="bottom"
                    android:text="0"
                    android:textSize="@dimen/Text_size_Traffic"
                    android:fontFamily="sans-serif-light"
                    android:textStyle="normal"

                    />
                <!--set font typeface ? right-->
                <TextView
                    android:id="@+id/subgmkb"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/subtraffic"
                    android:paddingLeft="@dimen/CircleProgressView_text_marginleft"
                    android:layout_alignBaseline="@+id/subtraffic"
                    android:textSize="@dimen/Text_size_GMKB"
                    android:text="M"
                    android:fontFamily="sans-serif-light"
                    android:textStyle="normal"
                    />
            </RelativeLayout>
        </LinearLayout>
    </FrameLayout>
