<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout  xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_weight="1">
    <TextView
        android:id="@+id/align_base"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/theme_temp_block_margin_top"
        android:singleLine="true"
        android:text="@string/comma"
        android:visibility="invisible"
        style="@style/custom_fixed_masthead_01"/>
    <TextView
        android:id="@+id/city_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingRight="@dimen/theme_italic_padding"
        android:drawableLeft="@drawable/clock_weather_icon_location_dark_s"
        android:layout_alignBaseline="@id/align_base"
        android:singleLine="true"
        android:ellipsize="marquee"
        android:visibility="gone"
        style="@style/custom_fixed_masthead_01"/>
    <FrameLayout 
        android:id="@+id/city_name_t"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/city_name"
        android:layout_alignBottom="@id/city_name"
        android:layout_alignLeft="@id/city_name"
        android:layout_alignRight="@id/city_name"/>
</RelativeLayout>
